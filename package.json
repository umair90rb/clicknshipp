{
  "name": "crm",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "18.15.0"
  },
  "scripts": {
    "start": "node ./build/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "server": "nodemon  --ignore client --exec babel-node src/index.js",
    "client": "npm start --prefix client",
    "clean:client": "rm -rf client/build",
    "clean:server": "rm -rf build && mkdir build",
    "build:client": "npm install --prefix client && npm run build --prefix client",
    "build:server": "npm i && babel -d ./build ./src/ -s",
    "build": "npm run clean:server && npm run clean:client && npm run build:client && npm run build:server && npm run db:migrate && npm run db:seed",
    "db:migrate": "npx sequelize-cli db:migrate",
    "db:migrate:undo": "npx sequelize-cli db:migrate:undo",
    "db:seed": "npx sequelize-cli db:seed:all",
    "heroku-postbuild": "NPM_CONFIG_PRODUCTION=false npm run build",
    "babel-version": "babel --version",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Umair",
  "license": "LICENSED",
  "dependencies": {
    "@aws-sdk/client-s3": "^3.616.0",
    "@sentry/cli": "^2.40.0",
    "@sentry/node": "^8.50.0",
    "axios": "^1.6.5",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bullmq": "^5.12.10",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.2.0",
    "dotenv": "^16.3.1",
    "exceljs": "^4.4.0",
    "express": "^4.18.2",
    "express-joi-validation": "^5.0.1",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "sequelize": "^6.34.0",
    "serve-index": "^1.9.1",
    "socket.io": "^4.7.5",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.13.0",
    "winston-daily-rotate-file": "^5.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.2",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/preset-env": "^7.23.2",
    "@babel/register": "^7.22.15",
    "nodemon": "^3.0.1"
  }
}
